<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>

    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <script src="~/Content/js/main.js"></script>

    <link rel="shortcut icon" href="~/Content/img/studenticon.png" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" name="fptstudent" content="studied">
    <link rel="stylesheet" href="~/Content/css/w3.css">
    <link rel="stylesheet" href="~/Content/css/w3themeorange.css">
    <link rel="stylesheet" href="http://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.3.0/css/font-awesome.min.css">
</head>
<body>
    <!-- Header -->
    <header class="w3-container w3-orange w3-padding" id="myHeader">
        <i onclick="w3_open()" class="fa fa-bars w3-xlarge w3-opennav"></i>
        <div class="w3-center">
            <h4 class="w3-btn w3-xlarge w3-theme-dark">FPT ADMIN WEB SITES</h4>
        </div>
    </header>

    <!-- dropdown*********************************************************** -->
    <div class="w3-center">
        <h2>Tùy Chọn</h2>
    </div>

    <div class="w3-row">
        <!--<div class="w3-col w3-container m2 w3-red"><a href="60basickanji/index.html" target="_blank">Free Half-Day Hanoi Walking Tour</a></div>-->
        <div style="cursor:pointer" class="w3-col w3-container m2 w3-red" onclick="location.href='@Url.Action("ManageStudent", "Home")'"><p>Manage Student</p></div>
        <div style="cursor:pointer" class="w3-col w3-container m2 w3-blue" onclick="location.href='@Url.Action("ManageSection", "Home")'"><p>Manage Section</p></div>
        <div class="w3-col w3-container m2 w3-blue-grey"><p>###</</p></div>
        <div class="w3-col w3-container m2 w3-teal"><p>###</p></div>
        <div class="w3-col w3-container m2 w3-yellow"><p>###</p></div>
        <div class="w3-col w3-container m2 w3-orange"><p>###</p></div>
    </div>

    <div class="container body-content">
        @RenderBody()
    </div>
    <!-- Footer -->
    <footer class="w3-container w3-theme-dark w3-margin-top">
        <hr>
        <div class="w3-col m3 ">
            <h4>Footer</h4>
        </div>
    </footer>

    <!-- Script for Side Navigation -->


    <script src="/lib/w3codecolors.js"></script>

    <div></div>
    

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>